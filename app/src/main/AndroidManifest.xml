<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="UjiKompetensiDTS"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UjiKompetensiDTS"
        tools:targetApi="31">
        <activity
            android:name=".NoteView"
            android:exported="false"
            android:label="Lihat Data"
            android:parentActivityName=".DataNote"
            android:theme="@style/Theme.MaterialComponents.DayNight.DarkActionBar"
            />
        <activity
            android:name=".AddEditNote"
            android:label="Input Data"
            android:parentActivityName=".NoteView"
            android:theme="@style/Theme.MaterialComponents.DayNight.DarkActionBar"
            android:exported="false" />
        <activity
            android:name=".DataNote"
            android:exported="false"
            android:label="Data Mahasiswa"
            android:parentActivityName=".Dashboard"
            android:theme="@style/Theme.MaterialComponents.DayNight.DarkActionBar" />
        <activity
            android:name=".Information"
            android:exported="false"
            android:label="Tentang Aplikasi"
            android:parentActivityName=".Dashboard"
            android:theme="@style/Theme.MaterialComponents.DayNight.DarkActionBar" />
        <activity
            android:name=".Register"
            android:exported="false" />
        <activity
            android:name=".Dashboard"
            android:exported="false"
            android:label="Dashboard"
            android:theme="@style/Theme.MaterialComponents.DayNight.DarkActionBar" />
        <activity
            android:name=".Login"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>